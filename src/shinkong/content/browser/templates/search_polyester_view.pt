<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
    xmlns:tal="http://xml.zope.org/namespaces/tal"
    xmlns:metal="http://xml.zope.org/namespaces/metal"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    xmlns:v-on="http://vuejs.org"
    lang="en"
    metal:use-macro="context/main_template/macros/master"
    i18n:domain="gct.content">
<metal:styleslot fill-slot="style_slot">
  <link href="++plone++shinkong.content/fancybox/dist/jquery.fancybox.min.css" rel="stylesheet" type="text/css">
</metal:styleslot>

<body>
<metal:content-core fill-slot="content-core">
<metal:content-core define-macro="content-core"
                    tal:define="toc context/table_of_contents|nothing;">


    <input type="search" id="subject" value="" placeholder="Search Subject Here..." >
    <button id='send'>send</button>

    <div id='result'></div>

<script src="++plone++shinkong.content/fancybox/dist/jquery.fancybox.min.js"></script>
<script>
$(function(){
    $('#send').click(function(){
        url = location.href.replace('search_polyester_view', 'search_polyester_result')
        val = $('#subject').val()
        data = {'subject': val}
        $.ajax({
            type: 'post',
            data: data,
            url: url,
            success: function(rep){
                $('#result').html(rep)
            }
        })
    })
})
</script>
</metal:content-core>
</metal:content-core>

</body>
</html>
